{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "CloudWatchLogs",
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": "arn:aws:logs:*:*:*"
    },
    {
      "Sid": "SQSAccess",
      "Effect": "Allow",
      "Action": [
        "sqs:ReceiveMessage",
        "sqs:DeleteMessage",
        "sqs:GetQueueAttributes"
      ],
      "Resource": "arn:aws:sqs:ap-northeast-2:287709190208:capstone-dev-video-processing"
    },
    {
      "Sid": "BatchSubmitJob",
      "Effect": "Allow",
      "Action": ["batch:SubmitJob", "batch:DescribeJobs", "batch:TagResource"],
      "Resource": [
        "arn:aws:batch:ap-northeast-2:287709190208:job-queue/capstone-dev-video-processing-queue",
        "arn:aws:batch:ap-northeast-2:287709190208:job-definition/capstone-dev-video-processor:*",
        "arn:aws:batch:ap-northeast-2:287709190208:job-queue/capstone-dev-video-analysis-gpu-queue",
        "arn:aws:batch:ap-northeast-2:287709190208:job-definition/capstone-dev-video-analysis-processor:*"
      ]
    }
  ]
}
